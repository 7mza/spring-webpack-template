{
  "name": "spring-webpack-template",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run format && npm run webpack",
    "webpack": "npx webpack",
    "clean": "npm cache verify",
    "clear": "npm run clean",
    "ncu": "npx ncu",
    "format": "npx prettier \"./**.{cjs,css,html,js,json,mjs,scss,ts}\" --write",
    "lint": "npx eslint"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.25.1",
    "@popperjs/core": "^2.11.8",
    "@types/bootstrap": "^5.2.10",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "bootstrap": "^5.3.5",
    "bootstrap-icons": "^1.11.3",
    "browserslist": "^4.24.4",
    "core-js": "^3.41.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.25.1",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-check-updates": "^18.0.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "purgecss-webpack-plugin": "^7.0.2",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  },
  "prettier": {
    "attributeSort": "ASC",
    "bracketSameLine": true,
    "plugins": [
      "prettier-plugin-organize-attributes"
    ],
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "singleAttributePerLine": true
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "not dead"
  ]
}
